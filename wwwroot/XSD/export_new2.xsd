						<xs:attribute name="НомерСФ" use="required" type="xs:string"/>
						<xs:attribute name="Проверка" use="required" type="xs:string"/>
						<xs:attribute name="СтавкаНДС" use="required" type="xs:byte"/>
						<xs:attribute name="ТипВагона" use="required" type="xs:byte"/>
						<xs:attribute name="ГодВыпуска" use="required" type="xs:short"/>
						<xs:attribute name="ТипРемонта" use="required" type="xs:byte"/>
						<xs:attribute name="ИзНихЗаРемонтКП" use="required" type="xs:byte"/>
						<xs:attribute name="СтоимостьБезНДС" use="required" type="xs:decimal"/>
						<xs:attribute name="СтоимостьУборки" use="required" type="xs:decimal"/>
