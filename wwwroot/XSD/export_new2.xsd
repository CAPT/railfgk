						<xs:attribute name="СтоимостьВзвешивания" use="required" type="xs:decimal"/>
						<xs:attribute name="ПлановоеНачалоРемонта" use="required" type="xs:string"/>
						<xs:attribute name="ПроверкаСобственником" use="required" type="xs:string"/>
						<xs:attribute name="ПробегПослеКапитального" use="required" type="xs:byte"/>
						<xs:attribute name="ПлановоеОкончаниеРемонта" use="required" type="xs:string"/>
						<xs:attribute name="ФактическоеОкончаниеРемонта" use="required" type="xs:string"/>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="ВЕРСИЯ" use="required" type="xs:byte"/>
		</xs:complexType>
	</xs:element>
</xs:schema>
