							<xs:element name="ЭЦП"/>
							<xs:element name="СотрудникиДепо"/>
							<xs:element name="ПредставителиДепо"/>
							<xs:element name="ПредставителиСобственника"/>
							<xs:element name="КоментарииКВагону"/>
							<xs:element name="РДВ"/>
							<xs:element name="АктВыбраковки"/>
							<xs:element name="ВУ36М"/>
							<xs:element name="МХ1"/>
							<xs:element name="МХ3"/>
							<xs:element name="ЛистКомплектации"/>
							<xs:element name="ВУ4М"/>
						</xs:sequence>
						<xs:attribute name="Код" use="required" type="xs:long"/>
